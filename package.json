{
  "name": "react-rollup-starter",
  "version": "1.0.3",
  "description": "react rollup demo",
  "repository": "dxmz/react-rollup-starter",
  "main": "lib/react-rollup-starter.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "serve": "serve -p 8080 example/dist",
    "dev-build": "rollup -w -c scripts/rollup.config.dev.js",
    "prepublishOnly": "npm run build",
    "clean-dist": "rimraf example/dist",
    "clean-lib": "rimraf lib",
    "start": "npm run clean-dist && cross-env NODE_ENV=development rollup -w -c scripts/rollup.config.dev.js",
    "build": "npm run clean-lib && cross-env NODE_ENV=production rollup -c scripts/rollup.config.prod.js",
    "deploy": "gh-pages -d example/dist",
    "publish-demo": "npm run dev-build && npm run deploy"
  },
  "keywords": [
    "react",
    "rollup",
    "rollup template",
    "react rollup",
    "rollup boilerplate",
    "npm",
    "publish",
    "react demo"
  ],
  "author": "daxiong",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.7.5",
    "@babel/core": "^7.7.5",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/polyfill": "^7.7.0",
    "@babel/preset-env": "^7.7.6",
    "@babel/preset-flow": "^7.7.4",
    "@babel/preset-react": "^7.7.4",
    "@babel/preset-stage-2": "^7.0.0",
    "@babel/runtime": "^7.7.6",
    "@rollup/plugin-node-resolve": "^6.0.0",
    "babel-eslint": "^10.0.3",
    "cross-env": "^6.0.3",
    "flow-bin": "^0.113.0",
    "gh-pages": "^2.1.1",
    "node-sass": "^4.13.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "rimraf": "^3.0.0",
    "rollup": "^1.27.11",
    "rollup-plugin-alias": "^2.2.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-copy": "^3.1.0",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-filesize": "^6.2.1",
    "rollup-plugin-image": "^1.0.2",
    "rollup-plugin-livereload": "^1.0.4",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-scss": "^1.0.2",
    "rollup-plugin-serve": "^1.0.1",
    "rollup-plugin-terser": "^5.1.3",
    "serve": "^11.2.0",
    "standard-version": "^7.0.1"
  },
  "dependencies": {}
}
